Proceso MiPrimeraApp
	Definir ciudadCapital Como Caracter;
	Definir pais Como Caracter;
		
	Escribir "Ingrese el nombre de una ciudad capital";
	leer ciudadCapital;
	
	Escribir "Ingrese el país de la ciudad capital";
	leer pais;
	
	Escribir "La ciudad ",ciudadCapital," es la capital del país ",pais;

FinProceso
